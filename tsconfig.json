{
    "compilerOptions": {
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "target": "ES5",
        "module": "esnext",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true,
        "paths": {
            "@common/embedding-context/*": ["./src/common/embedding-context/*"],
            "@common/event-manager/*": ["./src/common/event-manager/*"],
            "@common/events/*": ["./src/common/events/*"],
            "@common/iframe/*": ["./src/common/iframe/*"],
            "@experience/base-experience/*": ["./src/experiences/base-experience/*"],
            "@experience/console-experience/*": ["./src/experiences/console-experience/*"],
            "@experience/control-experience/*": ["./src/experiences/control-experience/*"],
            "@experience/dashboard-experience/*": ["./src/experiences/dashboard-experience/*"],
            "@experience/q-search-experience/*": ["./src/experiences/q-search-experience/*"],
            "@experience/generative-qna-experience/*": ["src/experiences/generative-qna-experience/*"],
            "@experience/internal-q-base-experience/*": ["./src/experiences/internal-q-base-experience/*"],
            "@experience/visual-experience/*": ["./src/experiences/visual-experience/*"]
        },
        "baseUrl": "."
    },
    "include": [
        "src",
        "tst"
    ]
}
